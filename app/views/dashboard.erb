<div id="prev-day"><a href="#" class="btn btn-primary">Previous Day</a></div>
  <div class="container">
    <h1 id="date">Lightning Talks - <%= date_format(@date) %></h1> 
    <table id="topics-date">
      <th>Topic</th>
      <th>Author</th>
      <% @days_talks.each do |talk| %>
         <tr>
          <td class='talk'><%= talk.topic %></td>
          <td class='author'><%= DBC::User.find(talk.speaker_id).name %></td>
        </tr>
      <% end %>
    </table>
    <a href="#" class="btn btn-primary btn-large">Add Talk</a>
  </div>
<div id="next-day"><a href="#" class="btn btn-primary">Next Day</a></div>


